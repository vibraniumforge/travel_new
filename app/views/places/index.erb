<p>app/views/places/index.erb</p>
<a href="/">Home</a>
<br>
<a href="/logout">Log Out</a>
<br>
<a href="/places/new">Create a New Place</a>
<br>
<hr>
<h1>Welcome, <%= Helpers.current_user(session).username %>!</h1>
<h3>All your places:</h3>
<br>
<hr>
<ol style="list-style:none">
<% @places.each do | place | %>
  <li>
    <p>Continent: <%= place.continent %> </p>
    <p>Country: <%= place.country %> </p>
    <p>State/Province: <%= place.state %> </p>
    <p>City: <%= place.city %></p>
    <p>Notes: <%= place.notes %> </p>
    <a href="/places/<%= place.id %>">Show Place</a>
    <br>
    <hr>
  </li>
  <% end %>
</ol>
  <br>
  <hr>
